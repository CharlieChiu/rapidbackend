<?xml version="1.0" encoding="UTF-8"?>
<beans 
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xmlns:tx="http://www.springframework.org/schema/tx"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd
        http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

<bean id="GetFeedSchema" class="com.socialutil.cell.appname.core.request.SocialRequestSchema">
  <property name="mandatorys" ref="GetFeedMandatoryParamGroup"/>
  <property name="optionals" ref="GetFeedOptionalParamGroup"/>
  <property name="schemaName" value="GetFeedRequestSchema"></property>
  <property name="command" value="GetFeed"></property>
</bean>

<bean id="GetFeedMandatoryParamGroup" class="com.socialutil.cell.appname.core.request.SocialRequestSchema.MandatoryParamGroup">
  <property name="paramList">
    <list>
      <ref bean="uid" />
      <ref bean="feedId" />
    </list>
  </property>
</bean>

<bean id="GetFeedOptionalParamGroup" class="com.socialutil.cell.appname.core.request.SocialRequestSchema.OptionalParamGroup">
  <property name="optionalParamList">
    <list>
      <ref bean="feedId" />
    </list>
  </property>
</bean>

<bean id="uid" class="com.socialutil.cell.appname.core.request.IntParam">
  <property name="name" value="uid"></property>
</bean>

<bean id="feedId" class="com.socialutil.cell.appname.core.request.IntParam">
  <property name="name" value="feedId"></property>
</bean>

</beans>